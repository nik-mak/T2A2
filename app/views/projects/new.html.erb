<h1> New Project </h1>
<%= form_with(model: @project, data: {turbo: false}) do |f| %>
  <fieldset class='mb-2'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset class='mb-2'>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </fieldset>
  <%# Need to remove this when user profiles have been created %>
 
  <fieldset>
    <%= f.submit class: 'btn btn-secondary btn-sm' %>
  </fieldset>
<% end %>
