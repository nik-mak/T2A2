<h1>
  Projects
  <% if user_signed_in? %>
    <%= link_to 'New Project', projects_new_path, class: "btn btn-primary btn-sm" %>
  <% end %>
</h1>
<% @projects.order('created_at DESC').each do |project| %>
  <% if project.status != true %>
    <p> <%= link_to project.name, project_path(project.id) %> </p>
  <% end %>
<% end %>
